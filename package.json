    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.1",
    "glob": "^8.0.1",
    "html-loader": "^3.1.0",
    "import": "^0.0.6",
    "mini-css-extract-plugin": "^2.6.0",
